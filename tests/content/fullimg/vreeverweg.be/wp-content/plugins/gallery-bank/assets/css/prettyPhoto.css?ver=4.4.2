.pp_top, .pp_bottom {
	height: 20px;
	position: relative
}

* html .pp_top, * html .pp_bottom {
	padding: 0 20px
}

.pp_top .pp_left, .pp_bottom .pp_left {
	height: 20px;
	left: 0;
	position: absolute;
	width: 20px
}

.pp_top .pp_middle, .pp_bottom .pp_middle {
	height: 20px;
	left: 20px;
	position: absolute;
	right: 20px
